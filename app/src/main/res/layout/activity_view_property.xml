<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_below="@+id/header"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:focusableInTouchMode="true"
        android:background="#f2f2f2"
        android:layout_height="match_parent"
        tools:context="com.eleganzit.brightlet.SignInActivity">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_alignParentTop="true"
            android:contentInsetRight="0dp"
            android:contentInsetStart="0dp"
            android:contentInsetLeft="0dp"
            app:contentInsetLeft="0dp"
            app:contentInsetStart="0dp"
            app:contentInsetRight="0dp"
            app:contentInsetEnd="0dp"
            android:background="#5a2e87"
            android:layout_height="?attr/actionBarSize">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="horizontal"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="25dp"
                    android:src="@drawable/path_633_2x"
                    android:layout_centerVertical="true"/>
                <com.eleganzit.brightlet.fonts.TextViewMuseo500
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="18sp"
                    android:id="@+id/txt"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:layout_centerInParent="true"
                    android:text="14 Groggles Street"
                    />
                <ImageView
                    android:id="@+id/edit"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/group_809_2x"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:text="Next"/>
            </RelativeLayout>
        </android.support.v7.widget.Toolbar>

    <LinearLayout
        android:layout_below="@+id/toolbar"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:focusableInTouchMode="true"
        android:background="#f2f2f2"
        android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_below="@+id/toolbar"
                android:background="#fff"
                android:layout_height="wrap_content"
                android:orientation="vertical">
        <LinearLayout
            android:id="@+id/first"
            android:layout_width="match_parent"
            android:layout_below="@+id/toolbar"
            android:background="#fff"
            android:layout_margin="10dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/one"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="bottom|right"
                        android:layout_gravity="bottom|right"
                        android:background="#f39200"
                        >
                        <com.eleganzit.brightlet.fonts.TextViewMuseo500
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom|right"
                            android:textColor="#fff"
                            android:paddingTop="7dp"
                            android:paddingBottom="7dp"
                            android:paddingLeft="12dp"
                            android:paddingRight="12dp"
                            android:text="TO LET"/>
                    </LinearLayout>
                </FrameLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:layout_marginTop="15dp"
                        android:orientation="vertical">

                            <com.eleganzit.brightlet.fonts.TextViewMuseo500
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="14 Groggles Street, DB5 4JF"
                                android:textColor="#5a2e87"
                                android:textSize="16sp"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="10dp"
                            android:gravity="center"
                            android:orientation="horizontal">
                            <com.eleganzit.brightlet.fonts.TextViewMuseo300
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="12sp"
                                android:layout_gravity="center"
                                android:textColor="#aaaaaa"
                                android:text="House"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_marginLeft="15dp"
                                android:layout_gravity="center"
                                android:background="#aaaaaa"
                                android:layout_height="18dp"></View>

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_marginLeft="15dp"
                                android:layout_gravity="center"
                                android:src="@drawable/group_288_3x"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="10dp"
                                android:text="x"/>

                            <com.eleganzit.brightlet.fonts.TextViewMuseo300
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:textSize="14sp"
                                android:layout_marginLeft="10dp"
                                android:text="2"
                                android:textColor="#181818"
                                android:padding="5dp"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_marginLeft="15dp"
                                android:layout_marginRight="5dp"
                                android:background="#aaaaaa"
                                android:layout_height="18dp"></View>

                            <ImageView
                                android:layout_width="16dp"
                                android:layout_height="16dp"
                                android:layout_marginLeft="15dp"
                                android:layout_gravity="center"
                                android:src="@drawable/image_14_3x"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="10dp"
                                android:textSize="12sp"
                                android:text="x"/>

                            <com.eleganzit.brightlet.fonts.TextViewMuseo300
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:textSize="14sp"
                                android:layout_marginLeft="10dp"
                                android:text="1"
                                android:textColor="#181818"
                                android:padding="5dp"/>

                            <View
                                android:layout_width="1dp"
                                android:layout_marginLeft="15dp"
                                android:layout_gravity="center"
                                android:background="#aaaaaa"
                                android:layout_height="18dp"></View>

                            <com.eleganzit.brightlet.fonts.TextViewMuseo300
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="15dp"
                                android:textSize="12sp"
                                android:layout_gravity="center"
                                android:textColor="#aaaaaa"
                                android:text="Furnished"/>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

    </LinearLayout>

        <LinearLayout
            android:layout_alignParentBottom="true"
            android:orientation="vertical"
            android:layout_marginTop="12dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fff">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                >

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    ><ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_weight="1"
                    android:scaleType="centerInside"
                    android:layout_gravity="center|left"
                    android:src="@drawable/group_810_3x"/>
                    <com.eleganzit.brightlet.fonts.TextViewMuseo500
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        android:layout_weight="7"
                        android:background="@android:color/transparent"
                        android:layout_gravity="left"
                        android:layout_marginLeft="10dp"
                        android:gravity="left"
                        android:text="Manage Tenants"
                        android:textSize="13dip" />
                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_weight="3"
                        android:scaleType="centerInside"
                        android:layout_gravity="center|end"
                        android:src="@drawable/path_581"/>

                </TableRow>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="15dp"
                    android:background="#EAEDED"
                    android:layout_marginBottom="2dp"></View>
                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    >
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:layout_gravity="center|left"
                        android:src="@drawable/group_824_3x"/>
                    <com.eleganzit.brightlet.fonts.TextViewMuseo500
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        android:layout_weight="7"
                        android:layout_marginLeft="10dp"
                        android:background="@android:color/transparent"
                        android:layout_gravity="left"
                        android:gravity="left"
                        android:drawablePadding="35dp"
                        android:text="Maintenance"
                        android:textSize="13dip" />
                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_weight="3"
                        android:scaleType="centerInside"
                        android:layout_gravity="center|end"
                        android:src="@drawable/path_581"/></TableRow>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="15dp"
                    android:background="#EAEDED"
                    android:layout_marginBottom="2dp"></View>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    ><ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_weight="1"
                    android:scaleType="centerInside"
                    android:layout_gravity="center|left"
                    android:src="@drawable/group_825_3x"/>
                    <com.eleganzit.brightlet.fonts.TextViewMuseo500
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        android:layout_weight="7"
                        android:layout_marginLeft="10dp"
                        android:background="@android:color/transparent"
                        android:layout_gravity="left"
                        android:drawablePadding="35dp"
                        android:gravity="left"
                        android:text="Image &amp; File Manager"
                        android:textSize="13dip" />
                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_weight="3"
                        android:scaleType="centerInside"
                        android:layout_gravity="center|end"
                        android:src="@drawable/path_581"/></TableRow>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="15dp"
                    android:background="#EAEDED"
                    android:layout_marginBottom="2dp"></View>
                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    >

                    <ImageView
                        android:layout_width="28dp"
                        android:scaleType="fitCenter"
                        android:layout_weight="1"
                        android:layout_height="28dp"
                        android:layout_gravity="center|left"
                        android:src="@drawable/e_3x"/>
                    <com.eleganzit.brightlet.fonts.TextViewMuseo500
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        android:layout_weight="7"
                        android:layout_marginLeft="10dp"
                        android:background="@android:color/transparent"
                        android:layout_gravity="left"
                        android:gravity="left"
                        android:text="Expenses"
                        android:textSize="13dip" />
                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_weight="3"
                        android:scaleType="centerInside"
                        android:layout_gravity="center|end"
                        android:src="@drawable/path_581"/>

                </TableRow>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="15dp"
                    android:background="#EAEDED"
                    android:layout_marginBottom="2dp"></View>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    >
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:layout_gravity="center"
                        android:src="@drawable/group_826_3x"/>
                    <com.eleganzit.brightlet.fonts.TextViewMuseo500
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        android:layout_weight="7"
                        android:layout_marginLeft="10dp"
                        android:drawablePadding="35dp"
                        android:background="@android:color/transparent"
                        android:layout_gravity="left"
                        android:gravity="left"
                        android:text="Documents Center"
                        android:textSize="13dip" />
                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_weight="3"
                        android:scaleType="centerInside"
                        android:layout_gravity="center|end"
                        android:src="@drawable/path_581"/>

                </TableRow>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="15dp"
                    android:background="#EAEDED"
                    android:layout_marginBottom="2dp"></View>

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    >

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_weight="1"
                        android:scaleType="centerInside"
                        android:layout_gravity="center"
                        android:src="@drawable/group_827_3x"/>
                    <com.eleganzit.brightlet.fonts.TextViewMuseo500
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="12dp"
                        android:layout_weight="7"
                        android:layout_marginLeft="10dp"
                        android:drawablePadding="35dp"
                        android:background="@android:color/transparent"
                        android:layout_gravity="left"
                        android:gravity="left"
                        android:text="Landlord Services"
                        android:textSize="13dip" />
                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_weight="3"
                        android:scaleType="centerInside"
                        android:layout_gravity="center|end"
                        android:src="@drawable/path_581"/>
                </TableRow>
            </TableLayout>


        </LinearLayout>
        </LinearLayout>


    </RelativeLayout>
    </ScrollView>